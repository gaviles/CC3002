package tarea;
import java.io.PrintStream;

public abstract class Histograma {
	protected String textData;
	protected HistogramProcessor histogramProcessor;
	
	public Histograma(){
		super();
	}
	
	public void show(PrintStream printStream ){
		printStream.print( this.getHistogram() );
	}
	public String getHistogram(){
		return histogramProcessor.getHistogram(textData);
	}
}
