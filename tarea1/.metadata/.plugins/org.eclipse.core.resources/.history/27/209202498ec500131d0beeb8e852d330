package tarea;

public class TextProcessor {
	
	public TextProcessor(){
	}
	public int[] processData(String string){
		
		String[] data = string.split(",");		
		
		int[] numbers = new int[data.length];
		
		for(int i = 0; i < data.length ; i++ ){
			numbers[i] = Integer.parseInt(data[i]);
		}
		return numbers;
	}
}
